{
  "expo": {
    "name": "하루정리",
    "slug": "dailyapp",
    "version": "1.0.2",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "resizeMode": "cover"
        }
      ],
      "expo-router"
    ],
    "icon": "./assets/images/dayicon.png",
    "scheme": "dailyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shinsieon.dailyapp",
      "usesAppleSignIn": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "78b4155e-ca07-408b-bc6e-387128315138"
      }
    },
    "android": {
      "package": "com.shinsieon.dailyapp"
    }
  }
}
