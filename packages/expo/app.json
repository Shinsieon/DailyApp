{
  "expo": {
    "name": "dailyapp",
    "slug": "dailyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dayicon.png",
    "scheme": "dailyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shinsieon.dailyapp",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dayicon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "78b4155e-ca07-408b-bc6e-387128315138"
      }
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
